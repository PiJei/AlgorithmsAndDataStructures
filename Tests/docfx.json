{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "."
        }
      ],
      "dest": "DocFx/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "DocFx/api/**.yml",
          "DocFx/api/index.md"
        ]
      },
      {
        "files": [
          "DocFx/articles/**.md",
          "DocFx/articles/**/toc.yml",
          "DocFx/toc.yml",
          "DocFx/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "DocFx/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "DocFx/apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
		  "bin/**",
          "DocFx/_site/**"
        ]
      }
    ],
    "dest": "DocFx/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}